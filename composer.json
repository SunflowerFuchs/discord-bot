{
	"name": "sunflowerfuchs/discord-bot",
	"description": "An easily extendable discord bot, written in PHP",
	"type": "library",
	"license": "GPL-3.0-or-later",
	"readme": "./README.MD",
	"version": "0.0.1",
	"autoload": {
		"psr-4": {
			"SunflowerFuchs\\DiscordBot\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"SunflowerFuchs\\DiscordBot\\Tests\\": "test/"
		}
	},
	"require": {
		"php": ">=8.1",
		"ext-json": "*",
		"ext-pdo": "*",
		"ext-sqlite3": "*",
		"react/event-loop": "^1.1.1",
		"ratchet/pawl": "^0.4.1",
		"guzzlehttp/guzzle": "^7.2.0",
		"symfony/options-resolver": "^5.3.7",
		"psr/log": "^1.1.4",
		"spatie/emoji": "^3.0",
		"pecee/pixie": "^4.15"
	},
	"suggest": {
		"gingteam/tiktok:dev-main": "Required for the TiktokPlugin"
	}
}
